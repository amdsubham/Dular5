{
  "expo": {
    "name": "Dular",
    "slug": "dular",
    "version": "18.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_big.png",
    "splash": {
      "image": "./assets/images/logo_big.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "dular",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_big.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Satoshi-Variable.ttf",
            "./assets/fonts/Satoshi-VariableItalic.ttf",
            "./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/Roboto/Roboto-Black.ttf",
            "./assets/fonts/Roboto/Roboto-BlackItalic.ttf",
            "./assets/fonts/Roboto/Roboto-Bold.ttf",
            "./assets/fonts/Roboto/Roboto-BoldItalic.ttf",
            "./assets/fonts/Roboto/Roboto-Italic.ttf",
            "./assets/fonts/Roboto/Roboto-Light.ttf",
            "./assets/fonts/Roboto/Roboto-LightItalic.ttf",
            "./assets/fonts/Roboto/Roboto-Medium.ttf",
            "./assets/fonts/Roboto/Roboto-MediumItalic.ttf",
            "./assets/fonts/Roboto/Roboto-Regular.ttf",
            "./assets/fonts/Roboto/Roboto-Thin.ttf",
            "./assets/fonts/Roboto/Roboto-ThinItalic.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to add pictures to your profile."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo_big.png",
          "color": "#EC4899"
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": false,
            "enableMinifyInReleaseBuilds": false
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app needs access to your photos to add pictures to your profile.",
        "NSCameraUsageDescription": "The app needs access to your camera to take photos for your profile."
      },
      "bundleIdentifier": "com.santali.dating"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_big.png",
        "backgroundColor": "#000000"
      },
      "backgroundColor": "#00000000",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json",
      "package": "com.santali.dating"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ed3addc-68dc-40e6-946e-4643353b2227"
      }
    }
  }
}
